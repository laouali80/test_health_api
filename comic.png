<span class="c1"># Interact with the API endpoint</span>
<span class="n">action</span> <span class="o">=</span> <span class="p">[</span><span class="s2">&quot;facilities&quot;</span><span class="p">,</span> <span class="s2">&quot;list&quot;</span><span class="p">]</span>
<span class="n">params</span> <span class="o">=</span> <span class="p">{</span>
    <span class="s2">&quot;api-key&quot;</span><span class="p">:</span> <span class="o">...</span><span class="p">,</span>
    <span class="s2">&quot;page&quot;</span><span class="p">:</span> <span class="o">...</span><span class="p">,</span>
